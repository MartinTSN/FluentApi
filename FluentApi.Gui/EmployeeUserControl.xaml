             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:FluentApi.Gui"
             mc:Ignorable="d"
             d:DesignHeight="450"
             d:DesignWidth="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="3*" />
            <ColumnDefinition Width="5*" />
        </Grid.ColumnDefinitions>
        <GroupBox Margin="10"
                  Header=" Oversigt ">
            <Grid>
                <DataGrid Margin="10"
                          x:Name="dataGridEmployees">

                </DataGrid>
            </Grid>
        </GroupBox>
    </Grid>
</UserControl>
